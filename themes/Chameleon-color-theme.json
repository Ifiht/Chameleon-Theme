{
	"type": "dark",
	"name": "Chameleon",
	"semanticHighlighting": true,
	"colors": {
		"editor.background": "#1C1C1C",
		"editorCursor.foreground": "#D3CFC1",
		"editor.foreground": "#F5F2F2",
		"editorWhitespace.foreground": "#D8D9D17A",
		"editor.lineHighlightBackground": "#282828",
		"editor.selectionBackground": "#FCFBF978",
		"statusBar.foreground": "#b9b9b9",
		"statusBar.background": "#030303",
		"statusBar.noFolderBackground" : "#0f1111",
    	"statusBar.debuggingBackground": "#091414",
		"statusBarItem.remoteBackground": "#555555"
	},
	"tokenColors": [
		{
			"settings": {
				"background": "#1C1C1C",
				"foreground": "#F5F2F2",
				"selectionForeground": "#FCFBF9B0",
				"selectionBorder": "#FCFBF9A6"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#DED6C7",
				"background": "#050505"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#DF00FC",
				"background": ""
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#0064FA",
				"background": "#003B8738",
				"fontStyle": " bold"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#F7A131",
				"background": "#FACD5221"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF8300",
				"background": "#FCB07233"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7A7979",
				"background": ""
			}
		},
		{
			"name": "Metablock",
			"scope": "meta.block",
			"settings": {
				"foreground": "#FA5300",
				"background": ""
			}
		},
		{
			"name": "Metaparens",
			"scope": [
				"meta.parens",
				"entity",
				"variable.parameter",
				"parameter"
			],
			"settings": {
				"foreground": "#FFA200",
				"background": "#FCE5800F",
				"fontStyle": " bold"
			}
		},
		{
			"name": "Metafunction",
			"scope": [
				"meta.function",
				"meta.function-call",
				"meta.function-call.ruby"
			],
			"settings": {
				"foreground": "#FFB400",
				"background": ""
			}
		},
		{
			"name": "Metabody",
			"scope": "meta.function.body",
			"settings": {
				"foreground": "#FFCD00",
				"background": "#FCE5800F"
			}
		},
		{
			"name": "Metacode",
			"scope": [
				"meta.function.body.code",
				"support",
				"keyword.control.clojure"
			],
			"settings": {
				"foreground": "#94ED05F5",
				"background": ""
			}
		},
		{
			"name": "Metasexpr",
			"scope": "meta.sexpr",
			"settings": {
				"foreground": "#FA5300",
				"background": "#FCE5800F"
			}
		},
		{
			"name": "MetaStor",
			"scope": [
				"storage",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#FA5300",
				"background": "#FCE5800F",
				"fontStyle": " bold"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"background": "#990000",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Diff Header",
			"scope": [
				"meta.diff.header",
				"meta.separator.diff",
				"meta.diff.index",
				"meta.diff.range"
			],
			"settings": {
				"background": "#2F33AB"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#F92672",
				"fontStyle": " italic"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#A6E22E",
				"fontStyle": " italic"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#967EFB",
				"fontStyle": " italic"
			}
		},
		{
			"name": "Metadirective",
			"scope": "meta.directive",
			"settings": {
				"foreground": "#7515B0",
				"background": "#0F081426",
				"fontStyle": " bold "
			}
		},
		{
			"name": "Metasymbol",
			"scope": "meta.symbol",
			"settings": {
				"foreground": "#D244F5",
				"background": "#F53FF50D"
			}
		},
		{
			"name": "Metagroup",
			"scope": "meta.group",
			"settings": {
				"foreground": "#006CFA"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#F5F3F2"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#DB1529",
				"background": "#B8373724",
				"fontStyle": " bold"
			}
		},
		{
			"name": "Metatag",
			"scope": "meta.tag",
			"settings": {
				"foreground": "#F5F3F2"
			}
		}
	]
}
